/* libs/components/header/index.wxss */
@import '/libs/css/iconfont.wxss';

.component-header{
  width: 100%;
  height: 40px;
  position: fixed;
  top: 0;
  box-shadow: 0 0 10px #b3b3b3;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 16px;
}
.component-header .iconfont{
  position: absolute;
  left: 10px;
  color: #000;
  line-height: 26px;
  font-size: 20px;
}